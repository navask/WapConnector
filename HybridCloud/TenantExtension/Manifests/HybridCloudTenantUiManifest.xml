<?xml version="1.0" encoding="utf-8"?>
<uiManifest>
  <!--Hybrid Cloud-->
  <extension name="HybridCloudTenantExtension"
             baseUri="~/Content/HybridCloudTenant">
    <scripts>
      <script src="~/Scripts/ViewModel/HW.ViewModels.QuickCreate.js" />
      <script src="~/Scripts/HybridCloudTenant.Controller.js" />
      <script src="~/Scripts/HybridCloudTenant.VirtualMachineTab.js" />
      <script src="~/Scripts/HybridCloudTenant.SettingsTab.js" />
      <script src="~/HybridCloudTenantExtension.js" />

      <!--Self registration-->
      <script src="~/extensions.data.js" />
    </scripts>
    <stylesheets>
      <stylesheet src="~/Styles/HybridCloudTenant.css"/>
    </stylesheets>
    <templates>
      <template name="customCreatePreview"
                src="~/Templates/Dialogs/CustomCreatePreview.html" />
      <template name="CreateFileShare"
                src="~/Templates/Dialogs/CreateFileShare.html" />
      <template name="VirtualMachineTab"
                src="~/Templates/Tabs/VirtualMachineTab.html" />
      <template name="VirtualMachineTabEmpty"
                src="~/Templates/Tabs/VirtualMachineTabEmpty.html" />
      <template name="settingsTab"
                src="~/Templates/Tabs/SettingsTab.html" />
    <template name="CreateVirtualMachine"
                src="~/Templates/Menu/VirtualMachineQuickCreate.html" />
    </templates>
  </extension>
</uiManifest>
